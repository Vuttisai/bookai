pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'npm install'
            }
        }
        stage('Test') {
            steps {
                sh 'echo testing'
            }
        }
        stage('Deliver') { 
            steps {
                sh 'npm run dev' 
                input message: 'Finished using the web site? (Click "Proceed" to continue)' 
                sh 'echo website running' 
            }
        }
    }
}
